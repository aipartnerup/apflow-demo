# Example environment variables for apflow-demo
# Copy this to .env and customize for your environment.

# Database connection (Postgres or DuckDB connection string)
DATABASE_URL="duckdb:///./data/agentflow_demo.duckdb"

# Demo mode and rate limiting
DEMO_MODE=true
RATE_LIMIT_ENABLED=true
RATE_LIMIT_DAILY_PER_USER=10
RATE_LIMIT_DAILY_LLM_PER_USER=1
MAX_CONCURRENT_TASK_TREES=10
MAX_CONCURRENT_TASK_TREES_PER_USER=1
RATE_LIMIT_DAILY_PER_IP=50

# Optional: override apflow tasks table name
#APFLOW_TASK_TABLE_NAME=apflow_tasks

# Notes:
# - After editing, run: `source .env` or restart services that read the file.
# - Recommended CLI: use the packaged wrapper so demo extensions are preloaded:
#     apflow-demo tasks all --limit 3
#   or run the app directly:
#     python -m apflow_demo.main
